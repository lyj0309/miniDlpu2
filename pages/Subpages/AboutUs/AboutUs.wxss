/* pages/setting-detail/AboutUs.wxss */
@media (prefers-color-scheme: dark) {
  page, image,open-data {
    background-color: #1f1f1f;
    filter: invert(1) hue-rotate(.5turn);
  }
  image,open-data{
    opacity: .75;
  }
}
/* pages/setting-detail/thanks.wxss */
page{
  width: 90%;
  margin: 20rpx 5%;
}

view.Stit {
  width: 100%;
  height: 120rpx;
  line-height: 120rpx;
  font-size: 3em;
  font-weight: 600;
  color: rgba(0, 0, 0, .8);
}

view.Stit text.Sver{
  padding-left: 15rpx;
  font-size: 0.35em;
  font-weight: normal;
  color: rgba(0, 0, 0, .8);
}
view.Sline{
  height: 1rpx;
  border-top: 1px rgba(0, 0, 0, .1) solid;
  padding-bottom: 10rpx;
  
}
view.Sinfo{
  font-size: 0.9em;
}
view.Sintro{
  text-indent: 2em;
  margin: 10rpx 0;
}
view.Stit2{
  width: 100%;
  height: 80rpx;
  line-height: 80rpx;
  font-size: 2em;
  font-weight: 600;
  color: rgba(0, 0, 0, .7);
}

view.space{
  height: 35rpx;
}